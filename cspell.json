{
  "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
  "version": "0.2",
  "language": "en,en-US",
  "allowCompoundWords": true,
  "words": [
    "ACMR",
    "antd",
    "archiver",
    "axios",
    "clsx",
    "defu",
    "dotenv",
    "ependencies",
    "execa",
    "iconify",
    "intlify",
    "lintstagedrc",
    "lucide",
    "pinia",
    "qrcode",
    "rollup",
    "shadcn",
    "sortablejs",
    "taze",
    "unplugin",
    "vite",
    "vitejs",
    "vitest",
    "vueuse"
  ],
  "ignorePaths": [
    "**/node_modules/**",
    "**/dist/**",
    "**/*-dist/**",
    "**/icons/**",
    "pnpm-lock.yaml",
    "**/*.log",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/__tests__/**"
  ]
}
